package client;

import maintenance.AccountMaintenance;
import db.DBHandler;

public class Client {

	public static void main(String[] args) {
		try(DBHandler dbh = new DBHandler(args[0], args[1])) {
			dbh.getTable("accountupdate");
			
			AccountMaintenance.updateAccounts("accounts", "accountupdate");
			
			} catch (Exception e) {
			e.printStackTrace();
		}
	}

}
